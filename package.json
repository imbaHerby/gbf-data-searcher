{
    "name": "gbf-data-searcher",
    "displayName": "GBF Data Searcher V2",
    "version": "1.0.9",
    "author": "Yuee",
    "description": "A tool to search GBF data",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/yueecui/gbf-data-searcher/"
    },
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview --port 4173",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\" \"src/**/*.css\"",
        "syncyarn": "npx syncyarnlock --save --keepPrefix"
    },
    "dependencies": {
        "@rushstack/eslint-patch": "^1.10.4",
        "@types/node": "^20.17.10",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vue/eslint-config-prettier": "^10.1.0",
        "@vue/eslint-config-typescript": "^14.1.4",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.17.0",
        "eslint-plugin-vue": "^9.32.0",
        "less": "^4.2.1",
        "naive-ui": "^2.40.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.7.2",
        "vite": "^6.0.4",
        "vue": "^3.5.13",
        "vue-tsc": "^2.1.10"
    },
    "packageManager": "yarn@4.5.3"
}

{
    "name": "gbf-data-searcher",
    "displayName": "GBF Data Searcher V2",
    "version": "1.0.6",
    "author": "Yuee",
    "description": "A tool to search GBF data",
    "repository": {
        "type": "git",
        "url": "https://github.com/yueecui/gbf-data-searcher/"
    },
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview --port 4173",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\" \"src/**/*.css\"",
        "syncyarn": "npx syncyarnlock --save --keepPrefix"
    },
    "dependencies": {
        "autoprefixer": "^10.4.7",
        "postcss": "^8.4.14",
        "tailwindcss": "^3.1.6",
        "vue": "^3.2.37"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.1.0",
        "@types/node": "^16.11.45",
        "@vitejs/plugin-vue": "^3.0.1",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.5.0",
        "eslint-plugin-vue": "^9.0.0",
        "less": "^4.1.3",
        "naive-ui": "^2.31.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "typescript": "~4.7.4",
        "vite": "^3.0.1",
        "vue-tsc": "^0.38.8"
    }
}
